<?xml version='1.0' encoding='utf-8'?>
<widget id="org.gipilab.simulateuremprunts" version="0.0.9" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="InAppBrowser">
        <param name="android-package" value="org.apache.cordova.inappbrowser.InAppBrowser" />
    </feature>
    <feature name="Notification">
        <param name="android-package" value="org.apache.cordova.dialogs.Notification" />
    </feature>
    <feature name="ActivityIndicator">
        <param name="android-package" value="org.apache.cordova.plugin.ActivityIndicator" />
    </feature>
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <allow-intent href="market:*" />
    <name>Simulateur d\'emprunts</name>
    <description>
	    Calculer le taux, la durée, l'échéance et le capital d'un emprunt.
    </description>
    <author email="thibault@gipilab.org" href="http://gipilab.org">
        Thibault Mondary, Laboratoire de Recherche pour le Développement Local
    </author>
    <icon src="www/bag.png" />
    <content src="index.html" />
    <access origin="http://gipilab.org/*" />
    <allow-navigation href="http://gipilab.org/*" />
</widget>
